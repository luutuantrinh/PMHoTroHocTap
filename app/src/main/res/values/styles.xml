<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="lilayoutedt">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <!--<item name="android:layout_margin">@dimen/maginall</item>-->
    </style>
    <style name="edtnumber">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/textsixe</item>

        <item name="android:editable">false </item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="button1_9">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">35dp</item>
        <item name="android:background">@drawable/button_color</item>
        <item name="android:textColor">@color/textbtn</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="button666">

        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textSize">30dp</item>
        <item name="android:background">@drawable/button_color</item>
        <item name="android:textColor">@color/textbtn</item>
        <item name="android:textAllCaps">false</item>
    </style>


    <style name="lilayoutman">

        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">@dimen/wight </item>
        <!--<item name="android:layout_margin">@dimen/maginall</item>-->
    </style>
    <style name="btnmenut">

        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">30sp</item>
        <item name="android:padding">4dp</item>
        <item name="android:layout_width">170dp </item>
        <item name="android:layout_height">170dp</item>
        <item name="android:layout_margin">10dp </item>
        <item name="android:background">@drawable/button_menu</item>
        <item name="android:textColor">@color/white</item>
        <!--<item name="android:layout_margin">@dimen/maginall</item>-->
    </style>

</resources>